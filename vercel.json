{
    "version": 2,
    "name": "oiiaoiiacat-fresh",
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate, max-age=0, private"
                },
                {
                    "key": "Pragma",
                    "value": "no-cache"
                },
                {
                    "key": "Expires",
                    "value": "-1"
                },
                {
                    "key": "Last-Modified",
                    "value": "Tue, 17 Jun 2025 12:00:00 GMT"
                },
                {
                    "key": "ETag",
                    "value": "\"fresh-v6.0-20250617\""
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                },
                {
                    "key": "Vary",
                    "value": "*"
                }
            ]
        }
    ]
} 