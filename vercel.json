{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.gif",
      "use": "@vercel/static"
    },
    {
      "src": "*.mp3",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(cat-static\\.gif|cat-spin\\.gif|spin\\.mp3)",
      "dest": "/$1"
    },
    {
      "src": "/(terms\\.html|privacy\\.html)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 