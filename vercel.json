{
  "name": "oiiaoiia-cat-game",
  "version": 2,
  "functions": {
    "api/click.js": {
      "runtime": "nodejs18.x"
    },
    "api/ranking.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 